import java.util.*;
public class Reverse {
    public static void main(String[] args){
       System.out.println("Enter a number: ");
       Scanner scn = new Scanner(System.in);
       int num = scn.nextInt();
       while(num > 0){
        int dig = num % 10;
        System.out.println(dig);
        num = num /10;
        }
    }
}
